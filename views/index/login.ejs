<% include ../partials/header.ejs%>

<div class = "container"> 
    <h1>Login</h1>

    <form action = "/login" method = "POST">
        <div class="form-group">
            <label for="usernameLoginInput">Username</label>
            <input name = "username" type="text" class="form-control" id="usernameLoginInput" placeholder="Your username" autocomplete="current-password">
        </div>

        <div class="form-group">
            <label for="passwordLoginInput">Password</label>
            <input name = "password" type="password" class="form-control" id="passwordLoginInput" placeholder="Your password" autocomplete="current-password">
        </div>
        
        <button type="submit" class="btn btn-outline-primary">Submit</button>

    </form>
</div>

<% include ../partials/footer.ejs%>